{
  "manifest_version": 3,
  "name": "My Extension Yonas",
  "version": "1.0.0",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["scripting", "activeTab", "tabs", "storage"],
  "icons": {
    "32": "ICON.png"
  },

  "host_permissions": ["http://*/*", "https://*/*"]
}
